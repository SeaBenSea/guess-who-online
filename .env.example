# Get these values from your Supabase project settings (https://supabase.com/dashboard)
NEXT_PUBLIC_SUPABASE_URL="your-project-url.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"

# Get this from your Supabase project settings -> API -> service_role key
# WARNING: Never expose this key in the browser or commit the actual key to version control
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Set a strong password for admin page access
# Make sure to use a strong password with special characters, numbers, and mixed case
NEXT_PUBLIC_ADMIN_PASSWORD="your-strong-admin-password" 
